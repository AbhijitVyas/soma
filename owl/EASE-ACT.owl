        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#PushingDown"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#PushingDown -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#PushingDown">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Pushing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#SupportingContact"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#overlappedOn"/>
