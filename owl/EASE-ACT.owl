    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#AbductiveReasoning -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#AbductiveReasoning">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Reasoning"/>
        <rdfs:comment>An action in which the Agent proceeds from some set of statements about a world, and attempts to obtain an explanation for these statements. This explanation is often an inferred cause, such as a final cause or intention. Further, it is often required that there be some guarantees that the explanation produced by AbductiveReasoning have some desirable property, such as being the simplest or most likely given the set of statements to explain. </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Actuating -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Actuating">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#NonCompositeAction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#during"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#EffectorContact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#DirectedMotion"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#during"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#EffectorContact"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#EffectorContact"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#contains"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#DirectedMotion"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#involvesEffector"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#PhysicalEffector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#simultaneous"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#DirectedMotion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#EffectorPositioning"/>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Navigation"/>
        <rdfs:comment>todo: think about use of tools. force events are generated between artifacts then. also not only the tool would be the &apos;salient artifact&apos; here.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Approaching -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Approaching">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Navigation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Avoiding"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Avoiding -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Avoiding">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Navigation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Catching -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Catching">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Grasping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#after"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#SupportingContact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#SupportingContact"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#before"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#GraspingMotion"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#PickingUp"/>
        <rdfs:comment>todo: would be nice to have &apos;Falling&apos; as force dynamics process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Closing -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Closing">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Actuating"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE.owl#ClosedState"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#after"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#OpenedState"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#overlappedBy"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#DirectedMotion"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE.owl#OpenedState"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#before"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#ClosedState"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#overlappedOn"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#DirectedMotion"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#overlappedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#OpenedState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#overlappedOn"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#ClosedState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Lifting"/>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Opening"/>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Squeezing"/>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#CompositeAction -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#CompositeAction">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#PhysicalAction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#hasAction"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#NonCompositeAction"/>
        <rdfs:comment>Any action that is named or conceptualized as an activity that consists of more than one non composite action, e.g. a sequence of non composite actions.

todo: (Mihai:) not touching this. We need a stronger idea of what makes a composite action, since just about any action is in some sense composite.</rdfs:comment>
        <rdfs:label>Composite action</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#DeductiveReasoning -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#DeductiveReasoning">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Reasoning"/>
        <rdfs:comment>An action in which the Agent, using general logical principles that it considers logically valid, applied to premises that it considers logically true, arrives at conclusions that it considers logically certain.

Deduction is often explained as starting from the &quot;general&quot; (some property X is known about all members of a set S), applying it to the &quot;specific&quot; (some Entity Y is known to belong to set S), to arrive at a specialization of the general property (X applies to Y).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Dreaming -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Dreaming">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#MentalAction"/>
        <rdfs:comment>Any form of re-processing episodic memories for long-term memory by natural or aritifical agents.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#Dropping -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Dropping">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#ReleasingGrasp"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#SupportingContact"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#after"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#EffectorContact"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#meets"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#SupportingContact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#startedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#ReleasingMotion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#LimbMotion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Placing"/>
        <rdfs:comment>The dropped object falls mainly under the influence of gravity. However, an agent may also drop something during navigation. The difference to &apos;Throwing&apos; is that there is no &apos;Limb motion&apos; which is a constitiuent of the action.

&apos;Dropping&apos; is intentional. Dropping by accident may not has a phase to release the grasp. It could be that the grasp was not strong enough and the objects &quot;slips&quot; away.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#EffectorPositioning -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#EffectorPositioning">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#NonCompositeAction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#LimbMotion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#involvesPlace"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#simultaneous"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#LimbMotion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#involvesEffector"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.ease-crc.org/ont/EASE.owl#PhysicalEffector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Navigation"/>
        <rdfs:label>Effector positioning</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#GraspLift -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#GraspLift">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#CompositeAction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#hasAction"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
