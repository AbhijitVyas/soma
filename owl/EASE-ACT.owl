        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#contains"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#Deformation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhase"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#Deformation"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#during"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#DirectedMotion"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#during"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#EffectorContact"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-ACT.owl#ThrowCatch -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#ThrowCatch">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#CompositeAction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#hasAction"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Catching"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#after"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Throwing"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#hasAction"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE-ACT.owl#Throwing"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#before"/>
                                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#Catching"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
