<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.ease-crc.org/ont/EASE-SAY.owl#"
     xml:base="http://www.ease-crc.org/ont/EASE-SAY.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:EASE="http://www.ease-crc.org/ont/EASE.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:knowrob="http://knowrob.org/kb/knowrob.owl#">
    <owl:Ontology rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl">
        <owl:imports rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/EASE-PROC.owl#interactsWith -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-PROC.owl#interactsWith"/>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#coversObject -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#coversObject">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#interactsWith"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#isCoveredByObject"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#hasInitialScene -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#hasInitialScene">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesObject"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#StateTransition"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#Scene"/>
        <rdfs:comment>A relation between StateTransitions and Scenes, which identifies the scene the transition starts from.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#hasTerminalScene -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#hasTerminalScene">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesObject"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#StateTransition"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#Scene"/>
        <rdfs:comment>A relation between StateTransitions and Scenes, which identifies the scene the transition is expected to end at.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#isCoveredByObject -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#isCoveredByObject">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#interactsWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#isMovedByAgent -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#isMovedByAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#interactsWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#movesObject -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#movesObject">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE-PROC.owl#interactsWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/EASE-OBJ.owl#Instrument -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-OBJ.owl#Instrument"/>
    


    <!-- http://www.ease-crc.org/ont/EASE-OBJ.owl#Patient -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-OBJ.owl#Patient"/>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#AgentRole -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#AgentRole">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A role often employed in ExecutableSchemas that generalises across thematic relations, such as agent or experiencer, which themselves abstract away from verb-specific roles.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#Assertion -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#Assertion">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#Illocution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#DeclarativeClause"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#CausalProcessRole -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#CausalProcessRole">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ExecutableSchematicTheory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Denotes some physical process that plays the role of causing a motion, e.g. motions, forces, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#CausedMotionTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#CausedMotionTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ImageSchemaTheory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Instrument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Patient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#AgentRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#CausalProcessRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#SourcePathGoalTheory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A schematic theory describing a situation in which an agent performs an action which causes an object to move along a certain path. A popular example being &quot;He sneezed the napkin off the table.&quot; (Goldberg 1995)

Let xA, xP be objects filling the agent, patient roles of this schema. Then one can infer that xA movesObject xP.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ClausalObject -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ClausalObject">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#Phrase"/>
        <rdfs:comment>A linguistic object that contains information which would be linguistically expressed as a clause.

A clause is a phrase containing a subject and a predicate.</rdfs:comment>
        <rdfs:label>Clausal object</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#Command -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#Command">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#Illocution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
                        <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ImperativeClause"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ContainmentTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ContainmentTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ControlTheory"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A functional relation describing that the location of some entity, the containee, is constrained to be within some region which is the interior of some other entity, the container.

This is also known as FunctionalControlInternal in GUM-4-space (Bateman et al. 2010).

Let xL, xR be objects filling the locatum, relatum roles of this schema. Then one can infer that xL isInsideOf xR.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ControlTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ControlTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#FunctionalSpatialSchemaTheory"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A description of functional-spatial configurations where one object controls another object&apos;s position in space, e.g. if a pot moves, then the popcorn contained therein moves, as well. Note that the objects do not need to be in direct contact.

Adopted from GUM-4-space (Bateman et al. 2010).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#CoverageTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#CoverageTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#FunctionalSpatialSchemaTheory"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A schematic theory of a functional relation between two objects such that one of them, the locatum, blocks access to the interior of the relatum.

Let xL, xR be objects filling the locatum, relatum roles of this schema. Then one can infer that xL coversObject xR.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#CoveringTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#CoveringTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ExecutableSchematicTheory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Instrument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Patient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A schematic theory of how an agent might use an instrument to prevent access to the interior of a patient.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#CrackingTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#CrackingTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ExecutableSchematicTheory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Instrument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Patient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A schematic theory of how an agent might go about inflicting damage to the surface of an object.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#DeclarativeClause -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#DeclarativeClause">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ClausalObject"/>
        <rdfs:comment>A clause which makes an assertion or declaration.</rdfs:comment>
        <rdfs:label>Declarative clause</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ESTSchemaTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ESTSchemaTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ImageSchemaTheory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#PhysicalObjectRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The ESTSchema constitutes the most basic schema that describes the existance of an entity, thereby having one Role, i.e. what entity is asserted to exist.

In SOMA, this schema will refer to the existence of physical objects. As such, it is not associated to a relation between two objects.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ExecutableSchematicTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ExecutableSchematicTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#SchematicTheory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#AgentRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Also known as &quot;executing schemas&quot; or &quot;x-schemas&quot;, these were defined by Bergen and Chang in their work &quot;Embodied Construction Grammar in Simulation-Based Language Understanding&quot; as:

&quot;Executing schemas, or x-schemas, are dynamic representations motivated in part by motor and perceptual systems (Bailey 1997; Narayanan 1997), on the assumption that the same underlying representations used for executing and perceiving an action are brought to bear in understanding language about that action. The
x-schema formalism is an extension of Petri nets (Murata 1989) that can model sequential, concurrent, and
asynchronous events&quot;

In SOMA, we do not restrict the formalism of ExecutableSchematicTheories; i.e. they don&apos;t have to be Petri Nets.

They maintain their role however as representations able to drive a simulation, at some level of abstraction, of an embodied action. This level of abstraction may be still fairly underspecified as in the case of the original x-schemas and as such not a plan that an agent could run in an actual physical environment without further information.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#FunctionalSpatialSchemaTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#FunctionalSpatialSchemaTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ImageSchemaTheory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#LocatumRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#RelatumRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The most general type of functional spatial relations.

Adopted from GUM-4-space (Bateman et al. 2010).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#GoalRole -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#GoalRole">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Denotes the destination of a route. Typically represented by a FunctionalSpatialSchema.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#Illocution -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#Illocution">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#CommunicationTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#Locution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A task which is executed by a Locution action.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ImageSchemaTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ImageSchemaTheory">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#SchematicTheory"/>
        <rdfs:comment>A theory that describes an image-schematic relationship between some entities.

Image schemas are considered as fundamental, pre-conceptual, building blocks of cognition. They were introduced to characterize how human beings are believed to organize and make sense of experience.

For SOMA, whereas the focus of executable schematic theories is to describe how an agent might act, image schematic theories focus on descriptions of how objects behave in the absence of, or after, an active intervention from an agent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#ImperativeClause -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#ImperativeClause">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ClausalObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"/>
                <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#StateTransition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"/>
                <owl:allValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#StateTransition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A clause which commands some agent to perform a task or bring about a state of affairs.</rdfs:comment>
        <rdfs:label>Imperative clause</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#InterrogativeClause -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#InterrogativeClause">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-SAY.owl#ClausalObject"/>
        <rdfs:comment>A clause which makes a request, typically information, of some agent.

Note that in a semantic sense such clauses always request information, but in a pragmatic sense they can be used to convey commands or requests for action, such as e.g. &quot;can you close the door?&quot; The question is not just a request for information about ability, but a request to perform a task.</rdfs:comment>
        <rdfs:label>Interrogative clause</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#LocatumRole -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#LocatumRole">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Denotes the object with primary focal prominence in a spatial or spatio-temporal schema. Terminological variants: Figure (Talmy 1983), Trajector (Langacker 1986).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#Locution -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#Locution">
        <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-ACT.owl#InterpretedAction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#LinguisticObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An action in which an agent uses some actuator for communication purposes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ease-crc.org/ont/EASE-SAY.owl#MixingTheory -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-SAY.owl#MixingTheory">
