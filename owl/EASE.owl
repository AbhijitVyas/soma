<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.ease-crc.org/ont/EASE.owl#"
     xml:base="http://www.ease-crc.org/ont/EASE.owl"
     xmlns:ease="http://www.ease-crc.org/ont/EASE.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:knowrob="http://knowrob.org/kb/knowrob.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">
    <owl:Ontology rdf:about="http://www.ease-crc.org/ont/EASE.owl">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/EASE.owl#symbol -->

    <owl:AnnotationProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#symbol">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ease-crc.org/ont/EASE.owl#after -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#after">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#follows"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#before"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&gt;</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities, expressing a &apos;sequence&apos; schema where one of the entities strictly ends before the other one.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#assistedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#assistedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
        <rdfs:label>assisted by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#before -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#before">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#precedes"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities, expressing a &apos;sequence&apos; schema where one of the entities strictly ends before the other one.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#coOccurs -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#coOccurs">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#overlaps"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A schematic relation between any events that also implies that one event is temporally contained in the other.

Sub-properties are used to distinct between different cases of event endpoint relations that hold for different types of co-occurance.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">co occurs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#contains -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#contains">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#coOccurs"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#during"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">di</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A contains B` means that A strictly starts before, and ends after B, i.e. B is wholly contained in A.</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">todo(DB): `contains` also applies to spatial domain, not only events</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#describesObject -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#describesObject">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
        <rdfs:label>describes object</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#describesSubject -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#describesSubject">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
        <rdfs:label>describes subject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#during -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#during">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#coOccurs"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">d</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A during B` means that B strictly starts before, and ends after A, i.e. A is wholly contained in B.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#finishedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#finishedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#coOccurs"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#finishes"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fi</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A finishes B` means that A ends exactly where B ends, and that B strictly starts before A.  As in &quot;I finish my day by taking a shower&quot;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#finishes -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#finishes">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#coOccurs"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">f</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A finishes B` means that A ends exactly where B ends, and that B strictly starts before A.  As in &quot;I finish my day by taking a shower&quot;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasAction -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasAction">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasConstituent"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/EASE.owl#CompositeAction"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/EASE.owl#PhysicalAction"/>
        <rdfs:label>has action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasCreated -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasCreated">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:label>has created</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasDestroyed -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasDestroyed">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:label>has destroyed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasInputParameter -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasInputParameter">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameter"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#isInputParameterFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
        <rdfs:label>has input parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasOutputParameter -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasOutputParameter">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameter"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#isOutputParameterFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
        <rdfs:label>has output parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasPhase -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasPhase">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasConstituent"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/EASE.owl#State"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>has phase</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasPhysicalComponent -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasPhysicalComponent">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasComponent"/>
        <rdfs:label>has physical component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasResult -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasResult">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
        <rdfs:label>has result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasSoftwareAgent -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasSoftwareAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
        <rdfs:label>has software agent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasState -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasState">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion"/>
        </owl:propertyChainAxiom>
        <rdfs:label>has state</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#hasStatus -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#hasStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality"/>
        <rdfs:label>has status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#involvesArtifact -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#involvesArtifact">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#isArtifactInvolvedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalArtifact"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artifact participation.</rdfs:comment>
        <rdfs:label>involves artifact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#involvesEffector -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#involvesEffector">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#isEffectorInvolvedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/EASE.owl#PhysicalEffector"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Effector participation.</rdfs:comment>
        <rdfs:label>involves effector</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#involvesPlace -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#involvesPlace">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#isPlaceInvolvedIn"/>
        <rdfs:label>involves place</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isAnsweredBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isAnsweredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
        <rdfs:label>is answered by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isArtifactInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isArtifactInvolvedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalArtifact"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artifact participation.</rdfs:comment>
        <rdfs:label>is artifact involved in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isAskedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isAskedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
        <rdfs:label>is asked by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isEffectorInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isEffectorInvolvedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/EASE.owl#PhysicalEffector"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Effector participation.</rdfs:comment>
        <rdfs:label>is effector involved in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isInputParameterFor -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isInputParameterFor">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParameterFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:label>is input parameter for</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isOutputParameterFor -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isOutputParameterFor">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParameterFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:label>is output parameter for</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#isPlaceInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#isPlaceInvolvedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:label>is place involved in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#meets -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#meets">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyPrecedes"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/EASE.owl#metBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">m</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities, expressing a &apos;sequence&apos; schema where one of the entities exactly ends where the other entity starts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/EASE.owl#metBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/EASE.owl#metBy">
