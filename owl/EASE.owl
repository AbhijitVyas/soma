        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mi</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities, expressing a &apos;sequence&apos; schema where one of the entities exactly ends where the other entity starts.</rdfs:comment>
    </owl:ObjectProperty>
    


