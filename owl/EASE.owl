    <!-- http://www.ease-crc.org/ont/EASE.owl#ReifiedRelation -->

    <owl:Class rdf:about="http://www.ease-crc.org/ont/EASE.owl#ReifiedRelation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Relation"/>
        <rdfs:subClassOf>
